<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="qiye.accountbook.userMapper"> 
  <select id="selectUser" parameterType="qiye.accountbook.domain.User" resultType="qiye.accountbook.domain.User">
	SELECT * FROM `user` WHERE name=#{name} AND password=#{password}
	</select>
</mapper>